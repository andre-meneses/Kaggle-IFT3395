    "import pandas as pd\n",
    "\n",
    "# Read the CSV file\n",
    "df = pd.read_csv('GNB.csv')\n",
    "\n",
    "# Add a new 'SNo' column with values ranging from 1 to the number of rows\n",
    "df['SNo'] = range(1, len(df) + 1)\n",
    "\n",
    "# Save the updated DataFrame to a new CSV file\n",
    "df.to_csv('NewGNB.csv', index=False)"
   ]
